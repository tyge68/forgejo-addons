{
    "name": "Forgejo Git Server",
    "version": "1.0",
    "slug": "forgejo",
    "description": "A lightweight, self-hosted Git server.",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "startup": "services",
    "boot": "auto",
    "ports": {
      "3000/tcp": 3000,
      "2222/tcp": 22
    },
    "webui": "http://[HOST]:[PORT:3000]",
    "map": [
      "config:rw",
      "ssl"
    ],
    "options": {
      "PUID": 1000,
      "PGID": 1000
    },
    "schema": {
      "PUID": "int",
      "PGID": "int"
    }
  }
  